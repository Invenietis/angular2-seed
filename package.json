{
  "name": "angular2-seed",
  "version": "1.0.0",
  "description": "A simple starter Angular2 project",
  "scripts": {
    "serve": "npm run http-server -- ./dist -a 0.0.0.0 -p 9000",
    "build": "npm run webpack -- --config config/webpack.prod.config.js  --progress --profile --bail",
    "build:aot": "cross-env BUILD_AOT=1 npm run webpack -- --config config/webpack.prod.config.js  --progress --profile --bail",
    "watch": "npm run build -- --watch",
    "server": "webpack-dev-server --config config/webpack.dev.config.js --inline --progress --watch --content-base src",
    "start": "npm run server",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "http-server": "http-server"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "^5.2.3",
    "@angular/animations": "^5.2.3",
    "@angular/compiler": "^5.2.3",
    "@angular/core": "^5.2.3",
    "@angular/forms": "^5.2.3",
    "@angular/http": "^5.2.3",
    "@angular/platform-browser": "^5.2.3",
    "@angular/platform-browser-dynamic": "^5.2.3",
    "@angular/router": "^5.2.3",
    "core-js": "^2.5.3",
    "reflect-metadata": "0.1.12",
    "rxjs": "5.5.6",
    "zone.js": "~0.8.20"
  },
  "devDependencies": {
    "@angular-devkit/build-optimizer": "^0.2.0",
    "copy-webpack-plugin": "^4.3.1",
    "cross-env": "^5.1.3",
    "http-server": "^0.11.1",
    "@angular/compiler-cli": "^5.2.3",
    "@types/node": "^9.4.1",
    "ng-router-loader": "^2.1.0",
    "ngc-webpack": "^4.1.2",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.4.1",
    "file-loader": "0.10.1",
    "css-loader": "^0.27.3",
    "sass-loader": "^6.0.6",
    "html-webpack-plugin": "^2.30.1",
    "raw-loader": "^0.5.1",
    "node-sass": "^4.7.2",
    "to-string-loader": "^1.1.5",
    "typescript": "~2.5.2",
    "@ngtools/webpack": "^1.9.7",
    "webpack": "~3.10.0",
    "webpack-dev-server": "~2.11.1",
    "webpack-merge": "~4.1.1",
    "webpack-md5-hash": "^0.0.6",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "extract-text-webpack-plugin": "^3.0.2",
    "tslib": "^1.9.0"
  },
  "keywords": [
    "Angular2",
    "angular2-seed"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/angular/angular2-seed.git"
  },
  "bugs": {
    "url": "https://github.com/angular/angular2-seed/issues"
  },
  "homepage": "https://github.com/angular/angular2-seed#readme"
}
